<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>rocp-project</display-name>
	<servlet>
		<servlet-name>UsersServlet</servlet-name>
		<servlet-class>com.revature.banking.servlets.UsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UsersServlet</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UsersServlet</servlet-name>
		<url-pattern>/users/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AccountServlet</servlet-name>
		<servlet-class>AccountServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AccountsServlet</servlet-name>
		<url-pattern>/accounts</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountServlet</servlet-name>
		<url-pattern>/accounts/:id</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountServlet</servlet-name>
		<url-pattern>/accounts/withdraw</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountServlet</servlet-name>
		<url-pattern>/accounts/deposit</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountServlet</servlet-name>
		<url-pattern>/accounts/transfer</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountServlet</servlet-name>
		<url-pattern>/accounts/status/:statusId</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AccountsServlet</servlet-name>
		<url-pattern>/passTime</url-pattern>
	</servlet-mapping>
